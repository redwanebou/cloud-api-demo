pipeline {
  agent { node { label 'master' }}
  tools { nodejs "redwaneJS" }
  stages {
    stage('Npm Install') {
      steps {
        sh '''
        npm install @angular/cli
        cd "AutoAPI"/ClientApp
        npm install
        '''
      }
    }

    stage('Build Production Mode') {
       steps {
          sh '''
          cd "AutoAPI"/ClientApp
          npm run "build"
          ''' 
       }
     }
   }
 }
